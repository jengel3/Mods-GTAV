@gta = angular
  .module('gta', [
    'restangular',
    'templates',
    'ngRoute'
  ])
  .run(->
    console.log 'gta running'
  )

@gta.config ($routeProvider, $locationProvider) ->
  $locationProvider.html5Mode true
  $routeProvider.when('/submissions',
    templateUrl: 'submissions/index.html'
    controller: 'SubmissionListCtrl' ).when('/submissions/:id',
    templateUrl: 'submissions/view.html'
    controller: 'SubmissionViewCtrl' ).otherwise redirectTo: '/'
  return