#contentoptions {
    width:186px;
    right:0;
    z-index: 2;
    padding-top:0;
    font-weight:normal;
    display: inline-block;
}

#contentoptions a {
    text-decoration:none;
    color:rgb(100,100,100);
    margin-left:0;
    padding-bottom:9px;
    padding-top:0;
    margin-top:0;
    vertical-align:1px;
}

#contentoptions ul {
    list-style:none;
    margin:0 auto;
    /*text-align:right;*/
    position:relative;
    display:inline-table;
    margin-left:0;
    padding-left:0;
    padding-right:4px;
    line-height:14px;

}

#contentoptions ul li {
    color:rgb(100,100,100);
    font-family:"arial", times, sans-serif;
    font-size:12px;
}

.downarrowsymbol {
    font-size:9px;
    vertical-align: top;
}

#contentoptions ul ul {
    display:none;
}

#contentoptions ul li:hover > ul {
    display:block;
}

#contentoptions ul ul {
    position: absolute; 
    top: 100%;
    background: white;
    border: 1px solid #000;
    overflow:hidden;
    line-height:normal;
    margin-right:4px;
}

#contentoptions ul ul li {
    position:relative;
    color:rgb(100,100,100);
    font-size:13px;
    display:block;
    white-space:nowrap;
}

#contentoptions ul ul li a{
    color:#fff;
    text-align:left;
    float:left;
    position:relative;
}

#contentoptions a:hover {
    color:rgb(100,100,100);
}

#contentoptions ul li:hover > li {
    background-color:transparent;

}

#contentoptions ul ul li:hover > a{
    color:rgb(50,50,50);
    background:rgb(240,240,240);
}

#contentoptions ul ul li > a {
    color:rgb(100,100,100);
    padding:4px 20px 5px 5px;
    width:100%;
    vertical-align:baseline;
}

