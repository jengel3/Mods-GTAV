
       
    body {
        margin: 0 auto;
        height: 100%;
        padding:0;
        background-color:#2a2b30;
        font-family: "Roboto",Helvetica Neue,Helvetica,sans-serif;
        /*font-family: Tahoma, Verdana, Segoe, sans-serif;*/
    } 
    html {
        margin:0;
        padding:0;
        height:100%;
    }
       
    .logoheader {
        height:35px;
        width:1314px;
        width:100%;
        /*border-bottom:1px solid #945c20;
        border-bottom:1px solid rgb(50,50,50);
        border-bottom:1px solid #23742b;*/
        border-width: 0;
        border-bottom-width: 1px;
        border-bottom-color: #23742b;
        border-style:solid;
        background-color: #131313;
        margin:0 auto;
        -webkit-box-shadow: 0px 0px 7px 0px rgba(0,0,0,0.75);
        -moz-box-shadow: 0px 0px 7px 0px rgba(0,0,0,0.75);
        box-shadow: 0px 0px 7px 0px rgba(0,0,0,0.75);
        position:relative;
        z-index: 5;
    }
       
    .inner_logoheader_wrap {
        width:1314px;
        height:100%;
        margin:0 auto;
    }
       
    .sitelogo {
        float:left;
        margin-top:4px;
    }

    .theme_dropdown_button {
        width:100px;
        float: right;
        color:white;
        margin-top:12px;
    }
       
    .headerwrap {
        width:1314px;
        height:128px;
        margin:0 auto;
        border-left: 1px solid #000;
        border-right: 1px solid #000;
        background-image: url(<%= image_path('GTAVheaderbackground.png') %>);
        background-color:#50b85a;
        position:relative;
        z-index:2;
    }
       
    .mobile_dropdown_menu_icon {
        display:none;
    }
       
    .mobile_dropdown_menu {
        display:none;
    }
       
    .gtavlogomobile {
        display:none;
    }
       
    .headericonswrap{
        width:100%;
        height:128px;;
    }
       
    .gtavlogowrap {
        display: inline-block;
        vertical-align: top;
        float:left;
        margin-top:5px;
        margin-left:25px;
        width:0px;
        display:none;
    }
       
    .headernavigationwrap {
        width:863px;
        width:100%;
        height:110px;
        height:100%;
        margin:0 auto;
        margin-top:23px;
        margin-top:0;
        font-size:0;
        display: inline-block;
        margin-left:-25px;
        margin-left:0;
        background-color:rgba(48,51,61,0.70);
        /*-webkit-border-radius: 5px;
        -moz-border-radius: 5px;
        border-radius: 5px;*/
    }
       
    .individual_navigation_icon_wrap {
        width:123px;
        height:100%;      
        display: inline-block;
        vertical-align: top;
    }
       
    .individual_navigation_icon_wrap_active {
        background-color:rgba(28,28,28,1);
    }
       
    .individual_navigation_icon_wrap:hover {
        background-color:rgba(0,0,0,0.25);
    }
       
    .top_navigation_bar {
        height:7px;
        border:0 none transparent;
    }
       
    .top_navigation_bar_active {
        background-color:#d98031;
        background-color:#e1e1e1;
        background-color:#42b84d;
    }
       
    .navigation_icon_img {
        margin-top:30px;
        margin-bottom:5px;
    }
       
    .navigation_icon_text {
        color:#f0f0f0;
        font-size:20px;

    }
       
    .headerbottomtransparentbar {
        width:100%;
        height:20px;
        background-color:#1c1c1c;
    }
       
    .pagewrap {
        width: 1336px;
        height:auto;
        margin:0 auto;
        text-align: center;
        overflow: hidden;
        position: relative;
    }
    .pagebodywrap {
        height: auto;
        width:1314px;
        background-color:#1c1c1c;
        border:1px solid black;
        border-top:none;
        border-bottom:none;
        margin: 0 auto;
    }
       
    .middlepage {
        background-color: #e1e1e1;
        width:860px;
        height:auto;
        text-align:center;
        margin:0 auto;
        margin-left:-4px;
        margin-right:-4px;
        margin-top:20px;
        border-top:2px solid #c2bfbe;
        display:inline-block;
        vertical-align: top;
        border-left: 2px solid #fff;
        border-right: 2px solid #fff;
        border-bottom: 2px solid #fff;
        border-top: 2px solid #fff;
        /*border-top:2px solid #e1e1e1;*/
        z-index:20;
        position:relative;
    }
       
    .sidebar {
        display:inline-block;
        width:205px;
        height:auto;
        margin-top:30px;
        /*background-color:#1c1c1c;*/
        background-color:#272727;
        z-index:10;
        position:relative;
        vertical-align: top;
        padding-bottom: 5000px;
        margin-bottom: -5000px;
    }
       .userprofilebox {
            width:100%;
           height:200px;
           background-color: #232323;
       }
       
            .userinfo_profilewrap {
                text-align:left;
            }
       
            .userinfo_profilewrap img {
                height:55px;
                width:55px;
                margin-top:14px;
                margin-left:14px;
            }
       
            .userinfo {
                display: inline-block;
                vertical-align: top;
                margin-top:16px;
                margin-left:6px;
            }
       
                .username {
                    color: #cbcbcb;
                    font-size: 18px;
                }
       
                .userdesc {
                    color: #939393;
                    font-size: 15px;
                }
       
            .userstats {
                margin-left:10px;
                margin-top:16px;
            }
       
                .userstats td {
                    padding-bottom:8px;
                    padding-right:5px;
                }
       
                .stat_title {
                    color: #939393;
                    font-size:15px;
                }
       
                .stat_amount {
                    color: #cbcbcb;
                    font-size:15px;
                    padding:2px 4px 2px 4px;
                    -webkit-border-radius: 2px;
                    -moz-border-radius: 2px;
                    border-radius: 2px;
                    border:1px solid transparent;
                }
                .accentcolor {
                    border: 1px solid #ff6666;
                }
       
       .subcategoriesbox {
            width:100%;
           height:400px;
           background-color:#272727;
       }
       
            .subcategory {
                width:100%;
                height:40px;
                text-align:left;
            }
                .subcategorydark {
                    background-color: #232323;
                }
       
                .activesubcategory {
                    background-color: #1a1a1a;
                    border-right:5px solid #737373;
                    width:198px;
                }
       
                .subcategory:hover {
                    background-color: #1a1a1a;
                    background-color: #1c1c1c;
                }
       
                .subcategory:first-child {
                    margin-top:8px;
                }
       
                .subcategory_circle {
                    width: 7px;
                    height:7px;
                    margin:16px 10px 0px 14px;
                    border-width:1px;
                    border-style:solid;
                    -webkit-border-radius: 50%;
                    -moz-border-radius: 50%;
                    border-radius: 50%;
                    display: inline-block;
                    
                    
                }
       
                .subcategorytext {
                    display: inline-block;
                    vertical-align: top;
                    font-size: 15px;
                    color:#939393;
                    margin-top:12px;
                }
                .activecategorytext {
                    color: #cbcbcb;
                }
                .subcategorytitle {
                    margin-left:20px;
                    font-size:15px;
                    color:#cbcbcb;
                }
       
       .filteringbox {
             width:100%;
           height:auto;
           background-color: #272727;     
       }
       
            input.search {
                display:inline-block;
                width:165px;
                height:30px;
                background-color:#232323;
                border: none;
                border-bottom:1px solid #4f4f4f;
                color:#6e6e6e;
                font-size:15px;
                padding-left:8px;
                padding-right:27px;
                outline:none;
                vertical-align:top;
                margin-top:20px;
                margin-bottom:-29px;
                
                }
            .searchwrap {
                background-color: #232323;
                height:90px;
                width: 100%;
            }

            .searchwrap ::-webkit-input-placeholder {
               font-style:italic;
               font-size:15px;
               color:#818080;
            }

            .searchwrap :-moz-placeholder { /* Firefox 18- */
               font-style:italic; 
               font-size:15px;
               color:#818080;
            }

            .searchwrap ::-moz-placeholder {  /* Firefox 19+ */
               font-style:italic; 
               font-size:15px;
               color:#818080;
            }

             .searchwrap :-ms-input-placeholder {  
               font-style:italic;  
               font-size:15px;
               color:#818080;
            }

            .searchbutton{
                display:inline-block;
                vertical-align: top;
                margin-left:135px;
                background-color: #ff6666;
                border:none;
            }
       
            .sortby_box_wrap {
                width: 100%;
                text-align: left;
                margin-bottom:30px;
            }
       
            .filtericon {
                margin-top:-5px;
            }
       
            .sortbyoptiontitle {
                color: #cbcbcb;
                margin-left:25px;
                margin-bottom:10px;
                margin-top:20px;
            }

            .sortby_options_wrap {
                height:auto;
                background-color: #232323;
                padding-top:6px;
                padding-bottom:6px;
            }
            .sortby_circle {
                width:7px;
                height:7px;
                display: inline-block;
                vertical-align: top;
                margin-top:4px;
                border-width:1px;
                border-style:solid;
                -webkit-border-radius: 50%;
                -moz-border-radius: 50%;
                border-radius: 50%;
                margin-right:5px;
            }
            .sortby_circle_active {
            }
       
            .sortbyoption {
                color:#7f7f7f;
                font-size: 15px;
                margin-left:28px;
                margin-top:5px;
                margin-bottom:5px;
            }
       
            .activefilteroption {
                color:#cbcbcb;
            }
       
       .todaysfeaturedbox {
              width:100%;
           height:400px;
           background-color: #272727;        
       }
       
            .sidebar_featured_today_title_wrap {
                margin-top:4px;
            }
       
            .sidebar_featured_today_title {
                font-size:18px;
                color:#cbcbcb;
                display: inline-block;
                vertical-align: top;
                margin-top:4px;
            }
       
            .sidebar_featured_today_thumb {
                height:92px;
                border:1px solid rgb(30,30,30);
                margin-top:5px;
                margin-bottom:10px;
                max-width:170px;
                opacity:0.8
            }

            .website_footer {
                width:1314px;
                height:auto;
                min-height:50px;
                background-color:#1c1c1c;
                border-left:1px solid #000;
                border-right:1px solid #000;
                border-bottom:1px solid #000;
                margin: 0 auto;
            }
            .pagination_wrap {
                background-color:#f2f2f2;
                width:860px;
                border-left:2px solid #fff;
                border-right:2px solid #fff;
                border-bottom:2px solid #fff;
                height:34px;
                margin:0 auto;
                text-align: center;
                margin-bottom:30px;
            }
            .pagination_prev_button {
                display: inline-block;
                vertical-align: top;
                text-decoration: none;
                color: #353535;
                font-weight:bold;
            }
            .pagination_prev_button .pagination_link:hover {
                color:#e34939;
            }
            .pagination_next_button .pagination_link:hover {
                color:#42b84d;
            }
            .pagination_next_button {
                display: inline-block;
                vertical-align: top;
                text-decoration: none;
                color: #353535;
                font-weight:bold;
            }
            .pagination_numbers_wrap {
                display: inline-block;
                vertical-align: top;
                font-size:0;
                position: relative;
                z-index:200;
            }
            .pagenumber {
                display: inline-block;
                vertical-align: top;
                height: 100%;
                width: 30px;
                font-size:16px;
                text-decoration: none;
                color: #353535;
                
                
            }
            .pagenumber .pagination_link:hover {
                color:#3979e3;
            }
            .activepagenumber {
                background-color:#fff;
                border-left:1px solid rgb(230,230,230);
                border-right:1px solid rgb(230,230,230);
            }
            .activepagenumber .pagination_link {
                color:#3979e3;
            }
            .pagination_link {
                text-decoration: none;
                color: #353535;
                margin-top:7px;
                vertical-align: top;
            }
            .bottom_footer_bar {
                width:1316px;
                height: 20px;
                background-color:black;
                margin: 0 auto;
                margin-bottom:50px;
                font-size: 12px;
                text-align:left;
                color: rgb(220,220,220);
            }
            .footer_bar_text {
                color:rgb(180,180,180);
                margin-left:8px;
                padding-top:3px;
            }
            .footer_bar_website_name {
                font-weight:bold;
                color:rgb(230,230,230);
                margin-right:5px;
            }

            #contentoptions {
                width:186px;
                right:0;
                z-index: 2;
                padding-top:0;
                font-weight:normal;
                display: inline-block;
            }

            #contentoptions a {
                text-decoration:none;
                color:rgb(100,100,100);
                margin-left:0;
                padding-bottom:9px;
                padding-top:0;
                margin-top:0;
                vertical-align:1px;
                }

            #contentoptions ul {
                list-style:none;
                margin:0 auto;
                /*text-align:right;*/
                position:relative;
                display:inline-table;
                margin-left:0;
                padding-left:0;
                padding-right:4px;
                line-height:14px;

                }

            #contentoptions ul li {
                color:rgb(100,100,100);
                font-family:"arial", times, sans-serif;
                font-size:12px;
                }

            .downarrowsymbol {
                font-size:9px;
                vertical-align: top;
            }

            #contentoptions ul ul {
                display:none;
                }

            #contentoptions ul li:hover > ul {
                display:block;
                }

            #contentoptions ul ul {
                position: absolute; 
                top: 100%;
                background: white;
                border: 1px solid #000;
                overflow:hidden;
                line-height:normal;
                margin-right:4px;
                }

            #contentoptions ul ul li {
                position:relative;
                color:rgb(100,100,100);
                font-size:13px;
                display:block;
                white-space:nowrap;
                }

            #contentoptions ul ul li a{
                color:#fff;
                text-align:left;
                float:left;
                position:relative;
                }

            #contentoptions a:hover {
                color:rgb(100,100,100);
                }

            #contentoptions ul li:hover > li {
                background-color:transparent;

                }

            #contentoptions ul ul li:hover > a{
                color:rgb(50,50,50);
                background:rgb(240,240,240);
                }

            #contentoptions ul ul li > a {
                color:rgb(100,100,100);
                padding:4px 20px 5px 5px;
                width:100%;
                vertical-align:baseline;
                }

    .accentborder {
        border-color: #ff6666;
        /*border-color: #41b64c;*/
    }
    
    .accentcolor {
        background-color: #ff6666;
        /*background-color: #41b64c;*/
    }



       
     @media(max-width:1350px) {
        
         .inner_logoheader_wrap {
            width:1110px;
         }
         .pagewrap {
            width:100%;
            width:1114px;
        }
         
        .headerwrap {
            width:1114px;
        }
         
        .gtavlogowrap {
            margin:18;
        }
         
        .gtavlogowrap img {
            width:90px;
            opacity:0.6;
        }
         
        .headernavigationwrap {
            margin-left:-108px;
            margin-left:0
        }
        .pagebodywrap {
            width: 1114px;
        }
         
        .middlepage {
            width:100%;
            max-width:672px;
        }
        .website_footer {
            width:1112px;
        }
        .pagination_wrap {
            width: 672px;
        }
        .bottom_footer_bar {
            width:1114px;
        }
     } 
       
       
       
    @media(max-width:1100px) {
        .inner_logoheader_wrap {
            width:900px;
         }
        /*.middlepage {
            width:100%;
            max-width:384px;
        }*/
         .pagewrap {
            width:890px;
        }
        
        .headerwrap {
            width:890px;
            
        }
        
        .gtavlogowrap {
            display: none;
        }
        .headernavigationwrap {
            margin:0;
        }
        .pagebodywrap {
            width: 890px;
        }
        .middlepage {
            width:100%;
            max-width:672px;
        }
        /*458px*/
        .sidebar_right {
            display: none;
        }
        .website_footer {
            width: 890px;
        }
        .pagination_wrap {
            margin-left:0;
            margin-left:218px;
            border-right: none;
            width:670px;
        }
        .bottom_footer_bar {
            width: 892px;
        }
    }

       
       
       
        
    @media(max-device-width:500px) {
        
        .inner_logoheader_wrap {
            display: none;
         }
        .sidebar {
            display: none;
        }
        
        .logoheader {
        }
        
        .headerwrap {
            width:100%;
            border:none;
        }
        
        .pagewrap {
            width:100%;
            min-width:500px;
        }
        
        .middlepage {
            width:100%;
            max-width:1000px;
            min-width:380px;
            min-height:100px;
            height:auto;
        }
        
        
        .headericonswrap {
            display:none;
        }
        
        .headerbottomtransparentbar {
            display:none;
        }
        
        .mobile_dropdown_menu_icon {
            display: block;
            float:right;
        }
        
        .mobilemenuicon {
            width:150px;
            height:150px;
            margin-top:10px;
            margin-right:20px;
        }
        
        .mobile_dropdown_menu {
            background-color: #ea9735;
            height:950px;
            width:100%;
            display:none;
            position:absolute;
            margin-top:180px;
            z-index:200;

            outline:6px solid #8f5a1c;
        }
        
        .mobile_menu_option {
            text-align: left;
            font-size:100px;
            color:#fff;
            margin-left:40px;
            margin-bottom:15px;
        }
        
        .mobile_menu_option:first-child {
            margin-top:25px;
        }
        
        .gtavlogomobile {
            float:left;
            margin:24px;
            opacity:0.8;
            display:block;
        }
        .website_footer {
            width: 100%;
        }
        .pagination_wrap {
            width: 100%;
            margin:0;
            height:90px;
            font-size:60px;
        }
        .pagenumber {
            font-size:45px;
            width: 50px;
        }
        .bottom_footer_bar {
            width:100%;
        }
        
    }