.downarrowsymbol {
    font-size:9px;
    vertical-align: top;
}

#contentoptions ul ul {
    display:none;
    margin-top:3px;
}

#contentoptions ul li:hover > ul {
    display:block;
}

#contentoptions ul ul {
    position: absolute; 
    top: 100%;
    background: white;
    border: 1px solid #000;
    overflow:hidden;
    line-height:normal;
    margin-right:4px;
}

#contentoptions ul ul li {
    position:relative;
    color:rgb(100,100,100);
    font-size:13px;
    display:block;
    white-space:nowrap;
}

#contentoptions ul ul li a{
    color:#fff;
    text-align:left;
    float:left;
    position:relative;
}

#contentoptions a:hover {
    color:rgb(100,100,100);
}

#contentoptions ul li:hover > li {
    background-color:transparent;

}

#contentoptions ul ul li:hover > a{
    color:rgb(50,50,50);
    background:rgb(240,240,240);
}

#contentoptions ul ul li > a {
    color:rgb(100,100,100);
    padding:4px 20px 5px 5px;
    width:100%;
    vertical-align:baseline;
}
.username_downarrowsymbol {
    font-size:9px;
    vertical-align: top;
    margin-left:3px;
}
.username #contentoptions {
    display: inline-block;
    vertical-align: top;
    width:auto;
}
li a.username {
    color:red;
}

.close_sidebar {
    display: none;
}

.content_error_box {
    width:800px;
    height:auto;
    margin-left:50px;
    background-color:#ff5a5f;;
    text-align:left;
    color:white;
    border-radius:3px;
}

.content_error_box ul {
    padding-top:8px;
    padding-bottom:8px;
    padding-right:8px;
}


/* ALERTS AND CONTACT */
.contact-popup {
    position: relative;
    background-color:#d8e1ef;
    padding: 10px 20px 20px 20px;
    width: auto;
    max-width: 500px;
    margin: 20px auto;
    border-radius: 3px;
    font-size:14px;
    font-weight:bold;
    color:rgb(50,50,50);
}

.contact-popup label {
    text-align: left;
}

.contactboxes {
    width:94%;
    margin-bottom:8px;
    background-color:rgb(253,253,253);
    outline: none;
    display: block;
    padding: 4px 8px;
    border: 1px dashed #a2badf;
    color: #3F3F3F;
    font-family: 'Droid Sans', Tahoma, Arial, Verdana sans-serif;
    font-size: 14px;
    -webkit-border-radius: 2px;
    -moz-border-radius: 2px;
    border-radius: 2px;
    -webkit-transition: background 0.2s linear, box-shadow 0.6s linear;
    -moz-transition: background 0.2s linear, box-shadow 0.6s linear;
    -o-transition: background 0.2s linear, box-shadow 0.6s linear;
    transition: background 0.2s linear, box-shadow 0.6s linear;
    resize: vertical;
    margin-top:3px;
}

.contactboxes:focus {
    background-color:rgb(255,255,255);
    border: dashed 1px #8ea8d1;
    -webkit-box-shadow: 2px 2px 7px #E8E8E8 inset;
    -moz-box-shadow: 2px 2px 7px #E8E8E8 inset;
    box-shadow: 2px 2px 7px #E8E8E8 inset;
}   

.contactsubmitbutton {
    height:25px;
    margin-top:5px;
}

/* Alerts */
.alert {
    width:auto;
    max-width:800px;
    height:auto;
    margin:0 auto;
    margin-top:20px;
    background-color:#ea473b;
    border:1px solid #d63a2f;
    color:white;
    padding:5px;
    padding-left:10px;
    text-align: left;
    -webkit-border-radius: 3px;
    -moz-border-radius: 3px;
    border-radius: 3px;
}

.alert.error {
    width: 100%;
    margin: 0px;
    max-width: 100%;
    overflow: auto;
}

.alert a.close-alert {
    float:right;
    color: #a6241b;
    text-decoration: none;
}
.notice {
    background-color:#31b95a;
    border:1px solid #198e3c;
}

.notice a.close-alert {
    color: #198e3c;
}

