    background: -webkit-linear-gradient(top,  #ed4a3d 0%,#e64337 100%,#2989d8 100%,#207cca 100%); /* Chrome10+,Safari5.1+ */
    background: -o-linear-gradient(top,  #ed4a3d 0%,#e64337 100%,#2989d8 100%,#207cca 100%); /* Opera 11.10+ */
    background: -ms-linear-gradient(top,  #ed4a3d 0%,#e64337 100%,#2989d8 100%,#207cca 100%); /* IE10+ */
    background: linear-gradient(to bottom,  #ed4a3d 0%,#e64337 100%,#2989d8 100%,#207cca 100%); /* W3C */
