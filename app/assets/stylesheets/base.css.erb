@media(max-width:1160px) {

    .mainpagewrap {
        width:900px;
        position: relative;
    }
    
    .innerlogoheader {
        width:100%;
    }
    
    .innerlogoheader img.logo {
        margin-left:15px;
    }
    .innerlogoheader img.twitter {
        margin-right:20px;
    }
    
    .mainpage {
        width:900px;
    }
    
    .right_sidebar {
        position:absolute;
        right:0;
        opacity:0.98;
        -webkit-box-shadow: -8px -1px 8px -8px rgba(0,0,0,0.56);
        -moz-box-shadow: -8px -1px 8px -8px rgba(0,0,0,0.56);
        box-shadow: -8px -1px 8px -8px rgba(0,0,0,0.56);
        z-index:300;
        display: none;
    }
    .right_sidebar_preview {
        display: block;
        background-color:#d8e1ef;
        position: absolute;
        right:0;
        width:15px;
        height: 100%;
        -webkit-box-shadow: -2px 0px 6px 0px rgba(0,0,0,0.12);
        -moz-box-shadow: -2px 0px 6px 0px rgba(0,0,0,0.12);
        box-shadow: -2px 0px 6px 0px rgba(0,0,0,0.12);
        z-index:400;
        border-left:1px solid #d1d8e3;
    }
    .showsidebar {
        display: block;
    }
    .showsidebarpreview {
        display: none;
    }
    .sidebar_expand_button {
        position: fixed;
        margin-top:130px;
        margin-left:2px;
    }
    .sorting_options_wrap {
        width: 900px;
        z-index:200;
        position: relative;
    }
    .navigation_bar_wrap ul {
        width: 900px;
    }
    .navigation_bar_wrap ul li {
        width: 97px;
    }
    .paginationfooter {
        width:900px;
        position: relative;
        z-index: 300;
    }
    .close_sidebar {
        position: absolute;
        left:3px;
        top:3px;
        color: #95a6b9;
        text-decoration: none;
        display: block;
    }

    

}

