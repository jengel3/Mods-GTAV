<%= render 'shared/submissions', submissions: @submissions %>
<%= content_for :scripts do %>
<script type="text/javascript">
	$(document).ready(function(){
		$('.submissiondate').hide();
		
		$(".modthumbnailwrap").on({
			mouseenter: function () {
				$('.submissiondate', this).stop().fadeIn(300);
			},
			mouseleave: function () {
				$('.submissiondate', this).stop().fadeOut(100);
			}
		});
    });
</script>
<% end %>